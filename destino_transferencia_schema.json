{
  "type": "object",
  "properties": {
    "result": {
      "type": "string",
      "enum": ["match", "unknown"],
      "description": "Indicates if the bank could be matched ('match') or if it's unknown."
    },
    "assetId": {
      "type": "string",
      "description": "Jira bank identifier."
    },
    "canonical": {
      "type": "string",
      "description": "Canonical name of the matched bank."
    },
    "match_method": {
      "type": "string",
      "enum": ["bcra", "alias", "text", "semantic"],
      "description": "Method used for the bank match."
    },
    "confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1,
      "description": "Confidence score between 0 and 1."
    },
    "explanation": {
      "type": "string",
      "description": "Reasoning or process explanation of the matching result."
    }
  },
  "required": ["result", "assetId", "canonical", "match_method", "confidence", "explanation"],
  "additionalProperties": false
}
